esSearch = item.name.toLowerCase().includes(searchQuery.value.toLowerCase()) ||\r\n                          item.description.toLowerCase().includes(searchQuery.value.toLowerCase())\r\n    const matchesCategory = selectedCategory.value === '' || item.category === selectedCategory.value\r\n    return matchesSearch && matchesCategory\r\n  })\r\n})\r\n\r\nconst editItem = (item) => {\r\n  // Implement edit functionality\r\n  console.log('Edit item:', item)\r\n}\r\n\r\nconst uploadExcel = (event) => {\r\n  // Implement Excel upload functionality\r\n  console.log('Upload Excel file:', event.target.files[0])\r\n}\r\n\r\nconst addItem = () => {\r\n  // Add new item to the list\r\n  const id = items.value.length + 1\r\n  items.value.push({ id, ...newItem.value })\r\n  showAddModal.value = false\r\n  // Reset form\r\n  newItem.value = {\r\n    name: '',\r\n    description: '',\r\n    category: '',\r\n    stockQuantity: 0,\r\n    minStockLevel: 0,\r\n    unitPrice: 0,\r\n    reorderLevel: 0\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.btn-primary {\r\n  @apply bg-blue-600 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded transition duration-300 ease-in-out flex items-center justify-center;\r\n}\r\n\r\n.btn-secondary {\r\n  @apply bg-gray-200 hover:bg-gray-300 text-gray-800 font-bold py-2 px-4 rounded transition duration-300 ease-in-out flex items-center justify-center;\r\n}\r\n</style>"],"mappings":";;;AA4HA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;AACnC,SAASC,QAAQ,EAAEC,UAAU,EAAEC,QAAQ,EAAEC,UAAU,QAAQ,iBAAiB;;;;;;;IAE5E,MAAMC,KAAK,GAAGN,GAAG,CAAC,CAChB;MAAEO,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,QAAQ;MAAEC,WAAW,EAAE,yBAAyB;MAAEC,QAAQ,EAAE,aAAa;MAAEC,aAAa,EAAE,EAAE;MAAEC,aAAa,EAAE,EAAE;MAAEC,SAAS,EAAE,MAAM;MAAEC,YAAY,EAAE;IAAG,CAAC,EACrK;MAAEP,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,YAAY;MAAEC,WAAW,EAAE,wBAAwB;MAAEC,QAAQ,EAAE,WAAW;MAAEC,aAAa,EAAE,EAAE;MAAEC,aAAa,EAAE,CAAC;MAAEC,SAAS,EAAE,MAAM;MAAEC,YAAY,EAAE;IAAG,CAAC,EACrK;MAAEP,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,gBAAgB;MAAEC,WAAW,EAAE,0BAA